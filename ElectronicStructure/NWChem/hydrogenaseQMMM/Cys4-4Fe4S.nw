basis "ao basis"
   Fe2 library 6-31g*
   Fe3 library 6-31g*
   S1 library 6-31+g*
   S2 library 6-31+g*
   C library 6-31+g*
   H library 6-31+g*
end

basis "cd basis"
   Fe2 library "Ahlrichs Coulomb Fitting"
   Fe3 library "Ahlrichs Coulomb Fitting"
   S1 library "Ahlrichs Coulomb Fitting"
   S2 library "Ahlrichs Coulomb Fitting"
   C library "Ahlrichs Coulomb Fitting"
   H library "Ahlrichs Coulomb Fitting"
end

charge -2.0
set tolguess 1e-07
set atomscf:tags_z S1 S2 Fe2 Fe3
set atomscf:z -1.0 -2.0 +2.0 +3.0

set geometry Cys+S
dft; mult 1; xc slater vwn_5; iterations 50; convergence density 1e-6;
     vectors atomic output Cys+S.mo; end
task dft

set geometry Fe1
dft; mult 6; xc slater vwn_5; iterations 100; convergence density 1e-6;
     vectors atomic output Fe1.mo; end
task dft

set geometry Fe2
dft; mult 5; xc slater vwn_5; iterations 300; convergence density 1e-6; smear;
     vectors atomic output Fe2.mo; end
task dft

set geometry Fe3
dft; mult -6; xc slater vwn_5; iterations 100; convergence density 1e-6;
     vectors input Fe1.mo output Fe3.mo; end
task dft

set geometry Fe4
dft; mult -5; xc slater vwn_5; iterations 300; convergence density 1e-6; smear;
     vectors input Fe2.mo output Fe4.mo; end
task dft

# Antiferromagnetic calculation
set geometry FS4_2+_AF
dft; mult 1; xc slater vwn_5; iterations 300; convergence density 1e-6; smear; 
     vectors input fragment Cys+S.mo Fe1.mo Fe2.mo Fe3.mo Fe4.mo \
     output FS4_2+_AF.movecs; end
     mulliken; print "final vectors" "mulliken"; end
task dft
